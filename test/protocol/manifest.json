{
  "manifest_version": 2,
  "version": "0.1",
  "name": "libdweb API tests",
  "description": "test libdweb APIs",

  "experiment_apis": {
    "test": {
      "schema": "../../src/test/test.json",
      "child": {
        "scopes": ["addon_child"],
        "paths": [["test"]],
        "script": "../../src/test/test.js"
      }
    },
    "protocol": {
      "schema": "../../src/toolkit/components/extensions/schemas/protocol.json",
      "child": {
        "scopes": ["addon_child"],
        "paths": [["protocol"]],
        "script": "../../src/toolkit/components/extensions/child/ext-protocol.js"
      },
      "parent": {
        "events": ["startup"],
        "scopes": ["addon_parent"],
        "paths": [["protocol"]],
        "script": "../../src/toolkit/components/extensions/parent/ext-protocol.js"
      }
    }
  },
  "background": {
    "scripts": ["test.js"]
  }
}
