{
  "manifest_version": 2,
  "version": "0.1",
  "name": "hyper@libdweb",
  "description": "Example of hypercore",
  "permissions": [],

  "experiment_apis": {
    "FileSystem": {
      "schema": "../../src/FileSystem/FileSystem.json",
      "child": {
        "scopes": ["addon_child"],
        "paths": [["FileSystem"]],
        "script": "../../src/FileSystem/client.js"
      },
      "parent": {
        "scopes": ["addon_parent"],
        "paths": [["FileSystem"]],
        "script": "../../src/FileSystem/host.js"
      }
    },
    "UDPSocket": {
      "schema": "../../src/UDPSocket/UDPSocket.json",
      "child": {
        "scopes": ["addon_child"],
        "paths": [["UDPSocket"]],
        "script": "../../src/UDPSocket/Socket.js"
      }
    }
  },
  "icons": {
    "48": "../mdns/logo-flyweb-grey.svg",
    "96": "../mdns/logo-flyweb-grey.svg"
  },
  "background": {
    "scripts": ["hyper.js"]
  }
}
